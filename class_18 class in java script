class pencilebox {
        1 reference
        constructor(
                name,
                size,
                color,
                ziplengthleft,
                ziplengthright,
                ziplengthtop,
                zipOpen,
        ){
                this.name = name;
                this.size = size;
                this.color = color;
                this.ziplength = {
                        left: ziplengthleft,
                        right: ziplengthright,
                        top: ziplengthtop,
                };
                this.zipOpen = zipOpen;
        }
        0 reference
        toggleZip(zipStatus){
                this.zipOpen = zipstatus;
        }

}
1 reference
